<% if(typeof questions!='undefined'){%>
    <% questions.forEach(question=>{%>
        <div class="card">
            <div class="card-body">
              <p class="card-text"><%=question.text%></p>
              <h6 class="card-text"><%=question.created%></h6>
              <div class="buttonGroup" style="display: flex;">
                <a href="/teacher/questions/<%=question.id%>" class="btn btn-primary">More</a>
                <a href="/teacher/questions/<%=question.id%>/answer" class="btn btn-warning">Answer</a>
              </div>
              
            </div>
          </div>          
    
<%})}%>