<% if(typeof questions!='undefined'){%>
  <% let questionArray = questions %>
  <%questions.forEach(question =>{%>
        <div class="card">
            <div class="card-body">
              <p class="card-text"><%=question.text%></p>
              <h6 class="card-text"><%=question.created%></h6>
              <div class="buttonGroup" style="display: flex;">
                <a href="/student/questions/<%=question.id%>" class="btn btn-primary">More</a>
                <a href="/student/questions/delete/<%=question.id%>" class="btn btn-raised btn-danger">Delete</a>
              </div>
            </div>
          </div>          
    
<%})}%>
<script>
  const confirmation = (questionId)=>{
  if(confirm("Do you want to delete the question")==true)
  {
    fetch('http://localhost:5000/student/questions/delete/'+questionId,{
      method:'GET',
      headers:{
        'Content-type':'application/json; charset=UTF-8'
      }
    });
  }
}
</script>